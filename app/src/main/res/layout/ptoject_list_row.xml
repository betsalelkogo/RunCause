<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    xmlns:tools="http://schemas.android.com/tools"
    android:background="?attr/selectableItemBackground"
    android:clickable="false">


        <TextView
            android:id="@+id/project_row_km_done_tv2"
            android:layout_width="61dp"
            android:layout_height="38dp"
            android:layout_marginStart="4dp"
            android:text="Done:"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/project_row_target_km_tv1"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.538" />

        <TextView
            android:id="@+id/project_row_target_km_tv"
            android:layout_width="130dp"
            android:layout_height="38dp"
            android:layout_marginStart="4dp"
            android:text="Run Target:"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/project_list_rv"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/project_list_rv"
            android:layout_width="88dp"
            android:layout_height="38dp"
            android:layout_marginStart="4dp"

            android:text="TextView"
            android:textSize="24sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0" />

        <TextView
            android:id="@+id/project_row_target_km_tv1"
            android:layout_width="48dp"
            android:layout_height="23dp"
            android:layout_marginStart="8dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/project_row_target_km_tv"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/project_row_km_done_tv1"
            android:layout_width="44dp"
            android:layout_height="22dp"
            android:layout_marginEnd="4dp"
            android:text="TextView"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@+id/project_row_km_done_tv2"
            app:layout_constraintTop_toTopOf="parent" />
</androidx.constraintlayout.widget.ConstraintLayout>
